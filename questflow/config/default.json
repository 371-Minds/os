{
  "appName": "QuestFlow-371OS",
  "version": "1.0.0",
  "description": "QuestFlow AI Workflow Automation Platform for 371OS",
  "main": "src/server.js",
  "type": "module",
  "packageManager": "bun@1.2.18",
  "scripts": {
    "start": "bun run serve",
    "dev": "bun --watch src/server.js",
    "serve": "bun src/server.js",
    "test": "bun test",
    "setup": "bun scripts/setup-371os.js",
    "test:unit": "bun test tests/unit",
    "test:integration": "bun test tests/integration",
    "docs": "bun scripts/generate-docs.js"
  },
  "keywords": ["ai", "workflow", "automation", "agents", "371os", "elizaos"],
  "author": "QuestFlow Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^18.0.0",
    "@types/express": "^4.17.17"
  },
  "environment": "development",
  "logging": {
    "level": "info",
    "format": "json"
  },
  "agents": {
    "defaultProvider": "elizaos",
    "timeout": 30000
  },
  "workflows": {
    "executionTimeout": 300000,
    "maxRetries": 3
  },
  "integration": {
    "elizaos": {
      "pluginsPath": "../os-workspace/packages",
      "agentsPath": "../os-workspace/agents"
    },
    "blockchain": {
      "registryAddress": "0x0000000000000000000000000000000000000000",
      "network": "ethereum"
    },
    "akash": {
      "deploymentUrl": "https://akash.network"
    },
    "postiz": {
      "apiKey": "YOUR_POSTIZ_API_KEY",
      "baseUrl": "https://api.postiz.com/v1"
    }
  }
}