{
  "name": "371-minds-os",
  "version": "1.0.0",
  "type": "module",
  "description": "371 Minds OS - Revolutionary Autonomous Agent Operating System with 97.6% Cost Reduction",
  "packageManager": "bun@1.2.18",
  "scripts": {
    "build": "bun nx run-many -t build",
    "test": "bun test",
    "lint": "bunx @biomejs/biome check --write .",
    "graph": "bun nx graph",
    "format": "bunx @biomejs/biome format --write .",
    "affected:build": "bun nx affected -t build",
    "affected:test": "bun test",
    "affected:lint": "bun nx affected -t lint",
    "start:dev": "concurrently \"bun run start:ceo\" \"bun run start:coordinator\"",
    "start:ceo": "bun --watch agents/ceo-mimi/start.js",
    "start:coordinator": "bun --watch agents/coordinator/start.js",
    "start:test-agent": "bun agents/test-agent/index.js",
    "start:monitoring": "./monitoring/health-check.sh",
    "quick-start": "powershell -ExecutionPolicy Bypass -File ./scripts/quick-start.ps1",
    "quick-start:bun": "powershell -ExecutionPolicy Bypass -File ./AB/scripts/bun-install.ps1",
    "deploy:akash": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-akash.ps1",
    "quick-start:bash": "./scripts/quick-start.sh",
    "deploy:akash:bash": "./scripts/deploy-akash.sh",
    "deploy:contracts": "cd contracts && bun hardhat run scripts/deploy.js --network sepolia",
    "monitor:akash": "./monitor-akash.sh",
    "health-check": "./monitoring/health-check.sh",
    "cost-analysis": "./scripts/cost-analysis.sh",
    "install:fast": "bun install",
    "install:npm-fallback": "npm install --legacy-peer-deps",
    "cognitive-engine:build": "bun nx build cognitive-engine",
    "cognitive-engine:test": "bun nx test cognitive-engine",
    "mongodb:start": "powershell -ExecutionPolicy Bypass -File ./scripts/start-mongodb.ps1",
    "mongodb:deploy-akash": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-mongodb-akash.ps1",
    "mongodb:connect": "node ./scripts/test-mongodb-connection.js",
    "system:start": "concurrently \"bun run mongodb:start\" \"bun run start:dev\"",
    "system:full-start": "powershell -ExecutionPolicy Bypass -File ./scripts/full-system-start.ps1",
    "deploy:github:uts": "bun nx run elizaos-plugin-universal-tool-server:deploy:github",
    "deploy:github:ceo": "bun nx run agents/ceo-mimi:deploy:github",
    "deploy:github:coordinator": "bun nx run agents/coordinator:deploy:github",
    "env:load": "powershell -ExecutionPolicy Bypass -File ../../scripts/load-env.ps1",
    "env:validate": "powershell -ExecutionPolicy Bypass -File ../../scripts/load-env.ps1 -Validate",
    "publish:all": "powershell -ExecutionPolicy Bypass -File ../../scripts/publish-packages.ps1 -Package all",
    "publish:plugins": "powershell -ExecutionPolicy Bypass -File ../../scripts/publish-packages.ps1 -Package elizaos-plugins",
    "publish:core": "powershell -ExecutionPolicy Bypass -File ../../scripts/publish-packages.ps1 -Package core-libraries",
    "publish:secure": "powershell -ExecutionPolicy Bypass -File ../../scripts/publish-packages.ps1 -Production",
    "deploy:with-secrets": "bun run env:load && bun run deploy:github:all",
    "deploy:github:all": "bun nx run-many -t deploy:github --all"
  },
  "keywords": [
    "ai-agents",
    "blockchain",
    "akash-network",
    "elizaos",
    "universal-tool-server",
    "autonomous-agents",
    "cost-optimization",
    "decentralized",
    "nx-workspace"
  ],
  "author": "371 Minds",
  "license": "MIT",
  "dependencies": {
    "@elizaos/core": "^1.5.2",
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@pieces.app/pieces-os-client": "^3.0.0",
    "@thi.ng/api": "^8.11.7",
    "@thi.ng/checks": "^3.6.9",
    "@thi.ng/errors": "^2.5.13",
    "@thi.ng/math": "^5.11.5",
    "@thi.ng/matrices": "^2.3.54",
    "@thi.ng/transducers": "^9.0.12",
    "@thi.ng/vectors": "^7.11.6",
    "cors": "^2.8.5",
    "crypto-browserify": "^3.12.1",
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "express": "^4.21.2",
    "fs-extra": "^11.3.1",
    "helmet": "^7.0.0",
    "ipfs-http-client": "^60.0.1",
    "mongodb": "^6.10.0",
    "mongoose": "^8.8.3",
    "posthog-node": "^4.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web3.storage": "^4.5.5",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nx/esbuild": "21.5.3",
    "@nx/js": "21.5.3",
    "@nx/node": "21.5.3",
    "@nx/react": "^21.0.0",
    "@nx/workspace": "^21.4.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/express": "^4.17.21",
    "@types/node": "^22.18.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "bun-types": "^1.2.21",
    "concurrently": "^8.0.0",
    "esbuild": "^0.19.2",
    "hardhat": "^2.19.0",
    "nexe": "^4.0.0-rc.6",
    "nx": "^21.4.1",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/371-Minds/os.git"
  },
  "bugs": {
    "url": "https://github.com/371-Minds/os/issues"
  },
  "homepage": "https://github.com/371-Minds/os#readme"
}
