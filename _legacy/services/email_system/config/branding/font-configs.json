  "portfolio_typography": {
    "name": "371minds Portfolio Typography System",
    "description": "Comprehensive typography system for all portfolio companies",
    "base_settings": {
      "base_font_size": "16px",
      "base_line_height": 1.6,
      "scale_ratio": 1.25,
      "font_weight_normal": 400,
      "font_weight_medium": 500,
      "font_weight_semibold": 600,
      "font_weight_bold": 700
    }
