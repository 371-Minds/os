{
  "name": "elizaos-characters",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/elizaos-characters/src",
  "projectType": "library",
  "tags": ["elizaos", "characters", "c-suite", "agents"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run build",
        "cwd": "packages/elizaos-characters"
      },
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run dev",
        "cwd": "packages/elizaos-characters"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun test",
        "cwd": "packages/elizaos-characters"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run lint",
        "cwd": "packages/elizaos-characters"
      }
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run lint:fix",
        "cwd": "packages/elizaos-characters"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run type-check",
        "cwd": "packages/elizaos-characters"
      }
    }
  }
}