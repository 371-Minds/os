{
  "name": "ping-agent",
  "version": "1.0.0",
  "type": "module",
  "description": "Ping Agent - Network connectivity testing utility agent with Nexe packaging support",
  "main": "src/index.ts",
  "keywords": [
    "ping-agent",
    "network-utility",
    "utility-agent",
    "371-minds",
    "nexe-compatible",
    "standalone-executable"
  ],
  "author": "371 Minds",
  "license": "MIT",
  "scripts": {
    "start": "bun --watch src/index.ts",
    "build": "bun nx build ping-agent",
    "package": "bun nx package ping-agent",
    "test": "bun test src/**/*.test.ts",
    "lint": "bunx @biomejs/biome check --write src/",
    "format": "bunx @biomejs/biome format --write src/"
  },
  "dependencies": {
    "@elizaos/core": "^1.5.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/node": "^22.18.0",
    "bun-types": "^1.2.21",
    "typescript": "^5.9.2"
  }
}