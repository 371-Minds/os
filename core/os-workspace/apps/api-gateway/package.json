{
  "name": "@371minds/api-gateway",
  "version": "1.0.0",
  "description": "Unified API Gateway for 371 OS - Phase 1 Foundation",
  "main": "src/main.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/main.ts",
    "dev": "bun --watch src/main.ts",
    "build": "bun build src/main.ts --outdir dist --target node",
    "test": "bun test src/**/*.test.ts",
    "lint": "bunx @biomejs/biome check --write src/"
  },
  "dependencies": {
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "http-proxy-middleware": "^2.0.6",
    "swagger-ui-express": "^5.0.0",
    "yaml": "^2.3.4",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/node": "^22.18.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "api-gateway",
    "authentication",
    "371-minds",
    "unified-api"
  ],
  "author": "371 Minds",
  "license": "MIT"
}
