  "typography_scale": {
    "h1": {
      "font_size": "2.5rem",
      "line_height": 1.2,
      "font_weight": 700,
      "margin_bottom": "1.5rem"
    },
    "h2": {
      "font_size": "2rem",
      "line_height": 1.3,
      "font_weight": 600,
      "margin_bottom": "1.25rem"
    },
    "h3": {
      "font_size": "1.75rem",
      "line_height": 1.4,
      "font_weight": 600,
      "margin_bottom": "1rem"
    },
    "h4": {
      "font_size": "1.5rem",
      "line_height": 1.4,
      "font_weight": 500,
      "margin_bottom": "0.875rem"
    },
    "h5": {
      "font_size": "1.25rem",
      "line_height": 1.5,
      "font_weight": 500,
      "margin_bottom": "0.75rem"
    },
    "h6": {
      "font_size": "1.125rem",
      "line_height": 1.5,
      "font_weight": 500,
      "margin_bottom": "0.625rem"
    },
    "body": {
      "font_size": "1rem",
      "line_height": 1.6,
      "font_weight": 400,
      "margin_bottom": "1rem"
    },
    "small": {
      "font_size": "0.875rem",
      "line_height": 1.5,
      "font_weight": 400
    },
    "caption": {
      "font_size": "0.75rem",
      "line_height": 1.4,
      "font_weight": 400
    }
  },
  "email_specific": {
    "subject_line": {
      "max_length": 50,
      "recommended_length": 30,
      "font_weight": 600,
      "avoid_all_caps": true
    },
    "preheader": {
      "max_length": 90,
      "recommended_length": 70,
      "font_weight": 400
    },
    "body_text": {
      "font_size": "16px",
      "line_height": 1.6,
      "max_line_length": "65ch",
      "paragraph_spacing": "1em"
    },
    "cta_button": {
      "font_size": "16px",
      "font_weight": 600,
      "text_transform": "none",
      "letter_spacing": "0.5px"
    }
  },
  "accessibility": {
    "minimum_font_size": "14px",
    "recommended_font_size": "16px",
    "minimum_line_height": 1.4,
    "recommended_line_height": 1.6,
    "color_contrast": {
      "minimum_ratio": 4.5,
      "preferred_ratio": 7.0
    }
  }
}
